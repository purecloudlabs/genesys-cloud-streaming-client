{
  "name": "purecloud-streaming-client",
  "version": "10.0.1",
  "description": "client for the PureCloud Streaming APIs (websocket/xmpp interface)",
  "repository": "https:github.com/purecloudlabs/purecloud-streaming-client",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/streaming-client.min.cjs",
  "browser": "dist/streaming-client.js",
  "files": [
    "src",
    "dist"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "build": "npm run build-umd && npm run build-cjs",
    "build-umd": "webpack && webpack --env.production",
    "build-cjs": "webpack --env.node && webpack --env.node --env.production",
    "unit-test": "NODE_ENV=test nyc --reporter=lcov --reporter=cobertura ava test/unit/*.js",
    "test": "semistandard && npm run unit-test && npm run report",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix",
    "report": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "watch-tests": "watch 'npm test' src test"
  },
  "semistandard": {
    "ignore": [
      "/.nyc_output",
      "/coverage",
      "/scripts",
      "/dist"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "files": [
      "test/unit/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true,
    "require": [
      "esm",
      "@babel/register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "",
  "dependencies": {
    "backoff-web": "^1.0.1",
    "debounce-promise": "^3.1.2",
    "limiter": "^1.1.0",
    "purecloud-streaming-client-webrtc-sessions": "^7.2.10",
    "stanza": "^11.0.0",
    "superagent": "^5.1.0",
    "uuid": "^3.0.1",
    "whatwg-fetch": "^3.0.0",
    "wildemitter": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "atob": "^2.1.2",
    "ava": "^2.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-polyfill": "^6.26.0",
    "btoa": "^1.2.1",
    "esm": "^3.2.25",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.8.0",
    "mock-socket": "^6.0.4",
    "nock": "^10.0.1",
    "nyc": "^14.1.1",
    "semistandard": "^13.0.1",
    "sinon": "^7.3.2",
    "snazzy": "^8.0.0",
    "watch": "^1.0.2",
    "webpack": "^4.35.3",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.5"
  },
  "false": {}
}
